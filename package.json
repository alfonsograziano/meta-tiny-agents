{
  "name": "meta-tiny-agents",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "dev": "tsx --env-file .env src/index.ts",
    "start-browser": "tsx src/browser.ts",
    "sync-rag": "tsx --env-file .env src/ragSync.ts",
    "start-rag-mcp": "tsx --env-file .env src/rag/rag-mcp.ts",
    "start-smart-fetch-mcp": "tsx --env-file .env src/smartFetch/index.ts",
    "start-client": "tsx --env-file .env src/client.ts",
    "start-server": "npm run migrate && tsx --env-file .env src/server.ts",
    "start-planner-agent": "tsx --env-file .env src/plannerAgent.ts",
    "migrate": "node db/create-db.js && db-migrate --migrations-dir db/migrations --config db/database.json up",
    "migrate-down": "db-migrate --migrations-dir db/migrations --config db/database.json down"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.12.1",
    "db-migrate": "^0.11.14",
    "db-migrate-pg": "^1.5.2",
    "dotenv": "^17.2.1",
    "openai": "^5.1.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.16.3",
    "playwright": "^1.55.0",
    "readline": "^1.3.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "user-agents": "^1.1.640",
    "zod": "^3.25.56"
  },
  "devDependencies": {
    "@types/node": "^22.15.30",
    "@types/pdf-parse": "^1.1.5",
    "@types/pg": "^8.15.5",
    "@types/user-agents": "^1.0.4",
    "@vitest/coverage-v8": "^3.2.2",
    "i": "^0.3.7",
    "npm": "^11.5.2",
    "tsx": "^4.19.4",
    "vitest": "^3.2.2"
  }
}
